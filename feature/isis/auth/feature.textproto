  path: "/network-instances/network-instance/protocols/protocol/isis/levels/level/authentication/config/enabled"
}
telemetry_path {
  path: "/network-instances/network-instance/protocols/protocol/isis/levels/level/authentication/state/enabled"
}

# Level csnp authentication
