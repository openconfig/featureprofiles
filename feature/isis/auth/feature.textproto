
id {
  name: "isis_auth"
  version: 1
}

# Global

# Global Authentication
config_path {
  path: "/network-instances/network-instance/protocols/protocol/isis/global/config/authentication-check"
}
telemetry_path {
  path: "/network-instances/network-instance/protocols/protocol/isis/global/state/authentication-check"
}

