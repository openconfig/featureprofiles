name: "arista-ceos-dutdut"
nodes: {
  name: "dut1"
  vendor: ARISTA
  model: "ceos"
  os: "eos"
  config: {
    image: "ceos:latest"
    config_path: "/mnt/flash"
    config_file: "startup-config"
    file: "arista_ceos.config"
    cert: {
      self_signed: {
        cert_name: "gnmiCert.pem"
        key_name: "gnmiCertKey.pem"
        key_size: 4096
      }
    }
  }
  services: {
    key: 22
    value: {
      name: "ssh"
      inside: 22
    }
  }
  services: {
    key: 6030
    value: {
      name: "gnmi"
      inside: 6030
    }
  }
  services: {
    key: 6040
    value: {
      name: "gribi"
      inside: 6040
    }
  }
}
