name: "nokia-ixia"
nodes: {
    name: "otg"
    vendor: KEYSIGHT
    version: "0.0.1-9999" # Please update this with the local version from ixiatg-configmap.yaml
    services: {
        key: 8443
        value: {
            name: "https"
            inside: 8443
        }
    }
    services: {
        key: 40051
        value: {
            name: "grpc"
            inside: 40051
        }
    }
    services: {
        key: 50051
        value: {
            name: "gnmi"
            inside: 50051
        }
    }
}

nodes: {
    name: "dut"
    vendor: NOKIA
    model: "ixr10"
    config: {
        file: "nokia_srl.json"
        image: "ghcr.io/nokia/srlinux:latest"
        cert: {
            self_signed: {
                cert_name: "kne-profile"
                key_name: "N/A"
                key_size: 4096
            }
        }
    }
    services:{
        key: 22
        value: {
            name: "ssh"
            inside: 22
        }
    }
    services:{
        key: 9337
        value: {
            name: "gnoi"
            inside: 57400
        }
    }
    services:{
        key: 9339
        value: {
            name: "gnmi"
            inside: 57400
        }
    }
    services:{
        key: 9340
        value: {
            name: "gribi"
            inside: 57401
        }
    }
    services:{
        key: 9559
        value: {
            name: "p4rt"
            inside: 9559
        }
    }
    interfaces: {
        key: "e1-1"
        value: {
            name: "ethernet-1/1"
        }
    }
    interfaces: {
        key: "e1-2"
        value: {
            name: "ethernet-1/2"
        }
    }
}
links: {
    a_node: "otg"
    a_int: "eth1"
    z_node: "dut"
    z_int: "e1-1"
}
links: {
    a_node: "dut"
    a_int: "e1-2"
    z_node: "otg"
    z_int: "eth2"
}
