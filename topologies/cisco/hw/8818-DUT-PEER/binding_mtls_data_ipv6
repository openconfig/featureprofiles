###################################### PEER 8808-FOX2733PDKP ########################################
#router static
# address-family ipv4 unicast
# vrf mgmtVrf
#  address-family ipv4 unicast
#   0.0.0.0/0 10.85.72.1
# !

#interface MgmtEth0/RP0/CPU0/0
# vrf mgmtVrf
# ipv4 address 10.85.72.47 255.255.255.0
# no shut
#!
#interface MgmtEth0/RP1/CPU0/0
# vrf mgmtVrf
# ipv4 address 10.85.72.48 255.255.255.0
# no shut
#!

#telnet: RP0 -  telnet 10.85.72.15 2001
#telnet: RP1 -  telnet 10.85.72.15 2002

##Exec server:
#ssh ott1lab-as2

## Consoe server:
# telnet 10.85.72.15
# Pass1: lab
# Pass2: lab

# Username: cafyauto
# Password: cisco123

## Management Virtual IP:
# ipv4 virtual vrf mgmtVrf address 10.85.72.49/24

duts {
  id: "peer"
  name: "8808_PEER"  # Change this to the device hostname.
  vendor: CISCO
  hardware_model: "CISCO-8808"

  options {
    insecure: false 
    skip_verify: false
    mutual_tls: true
    username: "cafyauto"
    password: "cisco123"
    trust_bundle_file: "$TRUST_BUNDLE_FILE"
    cert_file: "$CERT_FILE"
    key_file:  "$KEY_FILE"
  }

  # Options specific to gNMI.  Remove if not needed.
  gnoi{
    target: "10.85.72.49:8808"
  }
  gnmi {
    target: "10.85.72.49:8808"
  }
  gribi{
    target: "10.85.72.49:8808"
  }
  ssh{
    target: "10.85.72.49:22"
    skip_verify: true
  }
  p4rt{
    target: "10.85.72.49:8808"
  }

  # Before this binding can be used with a topology, add ports mapping
  # from its topology ID to the actual port name on the device.
   ports {
    id: "port16"
    name: "HundredGigE0/7/0/8"  # Change this to the actual port name.
    speed: S_100GB   
  }  
}

