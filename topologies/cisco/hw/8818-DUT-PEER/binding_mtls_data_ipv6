###################################### DUT 8818-FOX2714PNYN ######################################
### https://wwwin-github.cisco.com/B4Test/featureprofiles/tree/master/topologies/cisco/hw/8818-FOX2714PNYN ####
#router static
# address-family ipv4 unicast
#  5.0.0.0/8 5.18.0.1
#  5.5.14.197/32 5.18.0.1
#  5.24.27.1/32 5.18.0.1
#  200.1.1.1/32 150.1.1.1
#  223.255.253.253/32 5.18.0.1
#  223.255.254.0/24 5.18.0.1
#  223.255.254.254/32 5.18.0.1
# !
# vrf mgmtVrf
#  address-family ipv4 unicast
#   0.0.0.0/0 10.85.72.1
#  !
#  address-family ipv6 unicast
#   ::/0 2001:420:2841:3000::b001
#   1:1:1:1::/64 1:1:1:1::4
#  !


#interface MgmtEth0/RP0/CPU0/0
# description ott_ucs1_b4_ens4f0
# vrf mgmtVrf
# ipv4 address 10.85.72.50/24
# ipv6 address 1:1:1:1::1/64
# ipv6 address 2001:420:2841:3000::b006/116
#!
#interface MgmtEth0/RP1/CPU0/0
# description ott_ucs1_b4_ens4f1
# vrf mgmtVrf
# ipv4 address 10.85.72.51/24
# ipv6 address 1:1:1:1::2/64
#!

#telnet: RP0 -  telnet 10.85.72.15 2003
#telnet: RP1 -  telnet 10.85.72.15 2004

##Exec server:
#ssh ott1lab-as2

## Consoe server:
# telnet 10.85.72.15
# Pass1: lab
# Pass2: lab

# Username: cisco
# Password: cisco

## Management Virtual IP:
# ipv4 virtual vrf mgmtVrf address 10.85.72.52/24
# ipv6 virtual address vrf mgmtVrf 1:1:1:1::3/64
duts {
  id: "dut"
  vendor: CISCO
  hardware_model: "CISCO-8818"
  name: "8818_DUT"  # Change this to the device hostname.
  options {
    insecure: false 
    skip_verify: false
    mutual_tls: true
    username: "cafyauto"
    password: "cisco123"
    trust_bundle_file: "$TRUST_BUNDLE_FILE"
    cert_file: "$CERT_FILE"
    key_file:  "$KEY_FILE"
  }

  # Options specific to gNMI.  Remove if not needed.
  gnoi{
    # ott-ucs1-b4 ipv4->ipv6 data/in-band port
    target: "10.85.72.55:48819"
  }
  gnmi {
    target: "10.85.72.55:48819" 
  }
  gribi{
    target: "10.85.72.55:48819"
  }
  ssh{
    target: "10.85.72.55:48823"
    skip_verify: true
  }
  p4rt{
    target: "10.85.72.55:48819"
  }

