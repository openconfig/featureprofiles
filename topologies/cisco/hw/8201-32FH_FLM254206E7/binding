# ipv4 address 5.78.24.101 



options {
  username: "cafyauto"
  password: "cisco123"
}

duts {
  id: "dut"
  name: "R3-10.85.84.51:2014"  # Change this to the device hostname.
  config:{
    gnmi_set_file: "$BASE_CONF_PATH"
  }

  # Options inherited by all protocols on this device unless
  # overridden by individual protocols.  Remove if not needed.
  options {
    insecure: false 
    skip_verify: true
    username: "cafyauto"
    password: "cisco123"
  }

  # Options specific to gNMI.  Remove if not needed.
