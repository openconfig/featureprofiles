# requires killall -9 rinetd & rinetd after edit

options {
  username: "cafyauto"
  password: "cisco123"
  insecure: false 
  skip_verify: false
  trust_bundle_file: "$TRUST_BUNDLE_FILE"
  cert_file: "$CERT_FILE"
  key_file:  "$KEY_FILE"
}

duts {
  id: "dut1"
  name: "DUT1"  # Change this to the device hostname.
  # config:{
  #   gnmi_set_file: "$BASE_CONF_PATH"
  # }

  options {
    insecure: false 
    skip_verify: false
    mutual_tls: true
    username: "cafyauto"
    password: "cisco123"
  }

