options {
  username: "cisco"
  password: "cisco"
}

duts {
  id: "dut"
  name: "R4_churchill"  # Change this to the device hostname.
  config:{
    gnmi_set_file: "$BASE_CONF_PATH"
  }
  # telnet RP0: 10.85.84.53 2010
  # Mgmt RP0: 5.78.26.10
  # Rack Location : 22-18-26
  # PDU/outlets : 5.78.27.240/Outlets 5 & 6 
  # Exec server: ott2lab-as3
  # Options inherited by all protocols on this device unless
  # overridden by individual protocols.  Remove if not needed.
  options {
    insecure: false 
    skip_verify: true
    username: "cisco"
    password: "cisco"
  }

  # Options specific to gNMI.  Remove if not needed.
  gnoi{
    target: "10.85.84.159:56666"
  }
  gnmi {
    target: "10.85.84.159:56666"
  }
  gribi{
    target: "10.85.84.159:56666"
  }
  ssh{
    target: "10.85.84.159:46786"
  }
  p4rt{
    target: "10.85.84.159:56666"
  }

# Before this binding can be used with a topology, add ports mapping
