  otg {
    target: "10.85.17.225:40053" # Change this to the Ixia-c-grpc server endpoint.
    insecure: true
    timeout: 30
  }

  gnmi {
    target: "10.85.17.225:50053"  # Change this to the Ixia-c-gnmi server endpoint.
