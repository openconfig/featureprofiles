duts {
  id: "dut"
  name: "DUT"  # Change this to the device hostname.
   config:{
    gnmi_set_file: "$BASE_CONF_PATH"
  }

  # Options inherited by all protocols on this device unless
  # overridden by individual protocols.  Remove if not needed.
  options {
    insecure: false 
    skip_verify: true
    username: "cafyauto"
    password: "cisco123"
  }

  # Options specific to gNMI.  Remove if not needed.
  gnoi{
    target: "10.85.191.7:57779"
  }
  gnmi {
    target: "10.85.191.7:57779"
  }
  gribi{
    target: "10.85.191.7:57779"
  }
  ssh{
    target: "10.85.191.7:58000" 
  }
  p4rt{
   target: "10.85.191.7:58000"
  }

  # Before this binding can be used with a topology, add ports mapping
  # from its topology ID to the actual port name on the device.
  ports {
    id: "port1"
    name: "HundredGigE0/1/0/12"  # Change this to the actual port name.
  }
  ports {
    id: "port2"
    name: "HundredGigE0/1/0/13"  # Change this to the actual port name.
  }
  ports {
    id: "port3"
    name: "HundredGigE0/1/0/11"  # Change this to the actual port name.
  }
  ports {
    id: "port4"
    name: "HundredGigE0/1/0/10"  # Change this to the actual port name.
  }
  ports {
    id: "port5"
    name: "HundredGigE0/1/0/0"  # Change this to the actual port name.
  }
  ports {
    id: "port6"
    name: "HundredGigE0/1/0/1"  # Change this to the actual port name.
  }
  ports {
    id: "port7"
    name: "HundredGigE0/1/0/2"  # Change this to the actual port name.
  }
  ports {
    id: "port8"
    name: "HundredGigE0/1/0/3"  # Change this to the actual port name.
  }
}

