# interface MgmtEth0/RP0/CPU0/0
#  ipv4 address 5.18.11.12 255.255.0.0

#telnet: 
#RP0 -Â  telnet 10.85.84.47 2007

##Exec server:
#ssh ott-ucs4-b4

## Consoe server:
# telnet 10.85.84.47
# Pass1: lab
# Pass2: lab

# Username: cafyauto
# Password: cisco123

## Management interface RP0:
# ipv4 address 5.18.11.12

options {
  username: "cafyauto"
  password: "cisco123"
}

duts {
  id: "dut"
  vendor: CISCO
  hardware_model: "CISCO-8202"
  name: "8202_DUT"  # Change this to the device hostname.

  options {
    insecure: false 
    skip_verify: true
    username: "cafyauto"
    password: "cisco123"
  }

  # Options specific to gNMI.  Remove if not needed.
  gnoi{
    target: "10.85.84.225:48202"
  }
  gnmi {
    target: "10.85.84.225:48202"
  }
  gribi{
    target: "10.85.84.225:48202"
  }
  ssh{
    target: "10.85.84.225:48022"
  }
  p4rt{
    target: "10.85.84.225:48202"
  }

# Before this binding can be used with a topology, add ports mapping
# from its topology ID to the actual port name on the device.
  
  ports {
   id: "port1"
    name: "FourHundredGigE0/0/0/10"  # Change this to the actual port name.
    speed: S_400GB  
  }
  ports {
    id: "port2"
    name: "FourHundredGigE0/0/0/12"  # Change this to the actual port name.
    speed: S_400GB  
  }
  ports {
    id: "port3"
    name: "HundredGigE0/0/0/14"  # Change this to the actual port name.
    speed: S_100GB  
  }
  ports {
    id: "port4"
    name: "HundredGigE0/0/0/16"  # Change this to the actual port name.
    speed: S_100GB  
  }       
}
